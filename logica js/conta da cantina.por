programa {
  funcao inicio() {
  real  conta, conta_da_cantina
   inteiro cafe,coxinha,halls,doceDeLeite
   escreva("digite o valor do cafe: R$")
  leia(cafe)
  escreva("digite o valor do doceDeLeite: R$")
  leia(doceDeLeite)
   escreva("digite o valor da coxinha: R$")
   leia(coxinha)
   escreva("digite o valor do halls: R$")
   leia(halls)
   conta_da_cantina = cafe + coxinha + halls + doceDeLeite
   escreva("sua conta da cantina deu : " + conta_da_cantina)
  }
}
